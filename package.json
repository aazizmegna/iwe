{
  "name": "iwe-mobile",
  "version": "0.0.2",
  "author": "Ekamou",
  "homepage": "https://www.jhipster.tech",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e --port 8100",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write \"{,e2e/**/,src/**/}*.{js,json,html,md,ts,css,scss,yml}\" --loglevel silent"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@awesome-cordova-plugins/core": "^5.37.3",
    "@awesome-cordova-plugins/device-accounts": "^5.37.3",
    "@awesome-cordova-plugins/push": "^5.41.0",
    "@fortawesome/angular-fontawesome": "0.8.1",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@ionic-native/camera": "5.30.0",
    "@ionic-native/core": "^5.30.0",
    "@ionic-native/fcm": "^5.36.0",
    "@ionic-native/file": "^5.31.1",
    "@ionic-native/ionic-webview": "^5.33.1",
    "@ionic-native/push": "^5.36.0",
    "@ionic-native/splash-screen": "^5.30.0",
    "@ionic-native/status-bar": "^5.30.0",
    "@ionic/angular": "^5.5.2",
    "@ng-bootstrap/ng-bootstrap": "9.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@stomp/ng2-stompjs": "^8.0.0",
    "@types/lodash": "^4.14.168",
    "compare-func": "^2.0.0",
    "cordova": "^10.0.0",
    "cordova-browser": "6.0.0",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-camera": "^5.0.2",
    "cordova-plugin-file": "^6.0.2",
    "cordova-support-google-services": "1.3.2",
    "core-js": "^3.8.2",
    "ionic-image-loader": "^7.0.0-beta.2",
    "ionic-img-cache": "^1.3.2",
    "jquery": "^3.4.1",
    "lodash": "^4.17.21",
    "net": "^1.0.2",
    "ng-jhipster": "0.16.0",
    "ng2-stomp-service": "^1.2.2",
    "ngx-webstorage": "7.0.1",
    "npm-check-updates": "^10.2.5",
    "phonegap-plugin-multidex": "1.0.0",
    "phonegap-plugin-push": "^2.3.0",
    "rxjs": "~6.6.3",
    "rxjs-compat": "^6.6.3",
    "sockjs-client": "1.4.0",
    "stompjs": "^2.3.3",
    "tslib": "^2.1.0",
    "webstomp-client": "1.2.6",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "^11.0.7",
    "@angular/compiler": "~11.0.9",
    "@angular/compiler-cli": "~11.0.9",
    "@angular/language-service": "~11.0.9",
    "@ionic/angular-toolkit": "^3.0.0",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.21",
    "codelyzer": "^6.0.1",
    "cordova-android": "^9.1.0",
    "cordova-device-accounts-v2": "^2.0.8",
    "cordova-plugin-androidx": "^3.0.0",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-fcm-with-dependecy-updated": "^7.8.0",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "generator-jhipster": "^6.10.5",
    "generator-jhipster-ionic": "6.0.0",
    "html-loader": "1.3.2",
    "ionic-mocks-jest": "1.3.3",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "3.6.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "jest": "26.6.3",
    "jest-localstorage-mock": "2.4.6",
    "jest-preset-angular": "8.3.2",
    "karma": "~6.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "null-loader": "4.0.1",
    "onesignal-cordova-plugin": "^3.0.1",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "ts-loader": "8.0.14",
    "ts-node": "9.1.1",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.0.5",
    "webdriver-manager": "12.1.8"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": "Need camera access to take pictures",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "Need photo library access to get pictures from there",
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-androidx": {},
      "cordova-plugin-androidx-adapter": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-file": {},
      "cordova-plugin-fcm-with-dependecy-updated": {
        "ANDROID_DEFAULT_NOTIFICATION_ICON": "@mipmap/ic_launcher",
        "ANDROID_FCM_VERSION": "21.0.0",
        "ANDROID_FIREBASE_BOM_VERSION": "26.0.0",
        "ANDROID_GOOGLE_SERVICES_VERSION": "4.3.4",
        "ANDROID_GRADLE_TOOLS_VERSION": "4.1.0"
      },
      "onesignal-cordova-plugin": {},
      "cordova-device-accounts-v2": {},
      "phonegap-plugin-push": {
        "ANDROID_SUPPORT_V13_VERSION": "27.+",
        "FCM_VERSION": "17.0.+"
      }
    },
    "platforms": [
      "browser",
      "ios",
      "android"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/src/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "src/test.ts"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|@ionic-native|@ionic)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/*d.{ts}",
      "!**/*interface.{ts}",
      "!**/*module.{ts}",
      "!**/*main.{ts}",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageDirectory": "./public/coverage"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  },
  "description": "IWE - Your personal care partner"
}
